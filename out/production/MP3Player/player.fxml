<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="163.0" prefWidth="579.0" style="-fx-background-color: #222222;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <Label fx:id="label" alignment="CENTER" layoutX="-2.0" layoutY="-6.0" prefHeight="108.0" prefWidth="583.0" text="MP3 Player" textFill="#8c2424a0">
         <font>
            <Font name="Forte" size="84.0" />
         </font>
         <effect>
            <Blend />
         </effect>
      </Label>
      <ProgressBar fx:id="progressBar" layoutX="-4.0" layoutY="104.0" prefHeight="18.0" prefWidth="591.0" progress="0.0" />
      <Button fx:id="playButton" layoutX="-1.0" layoutY="122.0" mnemonicParsing="false" onAction="#playMedia" prefHeight="41.0" prefWidth="70.0" text="PLAY" textFill="#bc1010">
         <font>
            <Font name="Harrington" size="13.0" />
         </font>
         <effect>
            <SepiaTone level="0.33" />
         </effect>
      </Button>
      <ComboBox fx:id="speedBox" layoutX="349.0" layoutY="122.0" onAction="#changeSpeed" prefHeight="41.0" prefWidth="95.0" promptText="SPEED">
         <effect>
            <SepiaTone level="0.26" />
         </effect>
      </ComboBox>
      <Slider fx:id="volumeSlider" layoutX="444.0" layoutY="134.0" max="200.0" prefHeight="18.0" prefWidth="140.0" value="100.0" />
      <Button fx:id="pauseButton" layoutX="69.0" layoutY="122.0" mnemonicParsing="false" onAction="#pauseMedia" prefHeight="41.0" prefWidth="70.0" text="PAUSE" textFill="#bc1010">
         <font>
            <Font name="Harrington" size="13.0" />
         </font>
         <effect>
            <SepiaTone level="0.33" />
         </effect>
      </Button>
      <Button fx:id="resetButton" layoutX="139.0" layoutY="122.0" mnemonicParsing="false" onAction="#resetMedia" prefHeight="41.0" prefWidth="70.0" text="RESET" textFill="#bc1010">
         <font>
            <Font name="Harrington" size="13.0" />
         </font>
         <effect>
            <SepiaTone level="0.33" />
         </effect>
      </Button>
      <Button fx:id="previousButton" layoutX="209.0" layoutY="122.0" mnemonicParsing="false" onAction="#previousMedia" prefHeight="41.0" prefWidth="70.0" text="PREVIOUS" textFill="#bc1010">
         <font>
            <Font name="Harrington" size="10.0" />
         </font>
         <effect>
            <SepiaTone level="0.33" />
         </effect>
      </Button>
      <Button fx:id="nextButton" layoutX="279.0" layoutY="122.0" mnemonicParsing="false" onAction="#nextMedia" prefHeight="41.0" prefWidth="70.0" text="NEXT" textFill="#bc1010">
         <font>
            <Font name="Harrington" size="13.0" />
         </font>
         <effect>
            <SepiaTone level="0.33" />
         </effect>
      </Button>
   </children>
</AnchorPane>
